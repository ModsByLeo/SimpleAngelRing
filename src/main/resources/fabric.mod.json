{
  "schemaVersion": 1,
  "id": "simpleangelring",
  "version": "${version}",
  "name": "Simple Angel Ring",
  "description": "Adds an Angel Ring that grants creative flight. Angel Ring texture by LordDeatHunter.",
  "authors": [
    "ADudeCalledLeo",
    "LordDeatHunter"
  ],
  "contact": {
    "homepage": "https://github.com/Leo40Git/SimpleAngelRing",
    "sources": "https://github.com/Leo40Git/SimpleAngelRing",
    "issues": "https://github.com/Leo40Git/SimpleAngelRing/issues"
  },
  "license": "MIT",
  "icon": "assets/simpleangelring/icon.png",
